{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "jcore/nav-header",
  "version": "0.1.0",
  "title": "JCORE Navigation Header",
  "category": "navigation",
  "icon": "block-default",
  "description": "A wrapper block for navigation.",
  "attributes": {
    "scroll": {
      "type": "boolean",
      "default": false
    },
    "sticky": {
      "type": "boolean",
      "default": false
    }
  },
  "example": {},
  "supports": {
    "html": false,
    "layout": true,
    "align": ["wide", "full"],
    "background": {
      "backgroundImage": true,
      "backgroundSize": true
    },
    "color": {
      "background": true,
      "text": true
    }
  },
  "textdomain": "jcore",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
